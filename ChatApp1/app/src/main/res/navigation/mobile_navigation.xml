<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_message">

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.example.chatapp.chatroom.messages.MessageFragment"
        android:label="@string/chats"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.chatapp.chatroom.calendar.CalendarFragment"
        android:label="Calendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.chatapp.chatroom.settings.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings" />



</navigation>